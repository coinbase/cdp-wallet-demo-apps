# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_AUDIENCE=your-auth0-app-audience

# CDP Configuration
VITE_CDP_PROJECT_ID=your-cdp-project-id
VITE_CDP_BASE_PATH=https://api.cdp.coinbase.com/platform

# Wallet Configuration (optional - omit or leave empty to create no wallets)
# For Ethereum: set to 'eoa' for EOA wallets or 'smart' for Smart Account wallets
VITE_CDP_CREATE_ETHEREUM_ACCOUNT_TYPE=eoa
# For Solana: set to 'true' to create Solana wallets
VITE_CDP_CREATE_SOLANA_ACCOUNT=false
